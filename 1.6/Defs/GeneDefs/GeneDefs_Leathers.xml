<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<GeneDef>
		<defName>VRE_PigSkin</defName>
		<label>pigskin</label>
		<description>Carriers of this gene will yield pigskin when butchered. Wearing pigskin clothing by them is classed as wearing human leather clothing.</description>
		<iconPath>UI/Icons/Genes/Gene_PigSkin</iconPath>
		<biostatCpx>2</biostatCpx>
		<biostatMet>0</biostatMet>
		<displayCategory>Healing</displayCategory>
		<displayOrderInCategory>5000.2</displayOrderInCategory>
		<exclusionTags>
			<li>Leather</li>
			<li>Leathers</li>
			<li>AG_Leather</li>
		</exclusionTags>
		<modExtensions>
			<li Class="VEF.Genes.GeneExtension">
				<customLeatherThingDef>Leather_Pig</customLeatherThingDef>
				<defsTreatedAsHumanLeather>
					<li>Leather_Pig</li>
				</defsTreatedAsHumanLeather>
			</li>
		</modExtensions>
	</GeneDef>
</Defs>